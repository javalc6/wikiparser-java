{{TemplateDataHeader}}
<templatedata>
{
	"params": {
		"page": {
			"label": "Page title",
			"description": "The page that has been quickly deleted.",
			"example": "Main Page",
			"type": "wiki-page-name",
			"required": true
		},
		"reason": {
			"label": "Reason",
			"description": "The reason as to why the page was quickly deleted.",
			"type": "string"
		},
		"cat": {
			"label": "Deletion category",
