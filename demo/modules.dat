local function makeUrl(title, queryType, from)
	if title and title.fullUrl then
		if from then
			if queryType then
				local query = { [queryType] = from }
				return title:fullUrl(query, "https")
			else
				return title:fullUrl(nil, "https")
			end
		end
	end
